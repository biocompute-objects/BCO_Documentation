{
    "schema":{
    
  "type": "object",
  "title": "BioCompute",
  "description": "",
  "required": [
    "BCO_id",
    "type",
    "digital_signature",
    "bco_spec_version",
    "provenance_domain"
  ],
  "definitions": {
        "BCO_id": {
            "type": "string",
            "title": "BCO_id",
            "description": "",
            "default": "",
            "examples": [
                "https://github.com/biocompute-objects/BCO_Spec_V1.2/blob/master/HCV1a.json"
            ],
            "readOnly": true
        },
        "contributor": {
            "type": "object", 
            "title": "contributor", 
            "description": "", 
            "required": [
                "contribution"
            ], 
            "properties": {
                "name": {
                    "type": "string", 
                    "title": "name", 
                    "description": "", 
                    "examples": [
                        "Charles Hadley King"
                    ]
                }, 
                "affiliation": {
                    "type": "string", 
                    "title": "affiliation", 
                    "description": "", 
                    "examples": [
                        "George Washington University"
                    ]
                }, 
                "email": {
                    "type": "string", 
                    "title": "email", 
                    "description": "", 
                    "examples": [
                        "hadley_king@gwu.edu"
                    ], 
                    "format": "email"
                }, 
                "contribution": {
                    "type": "array", 
                    "title": "contribution", 
                    "description": "", 
                    "items": {
                        "type": "string", 
                        "enum": [
                            "authoredBy", 
                            "contributedBy", 
                            "createdAt", 
                            "createdBy", 
                            "createdWith", 
                            "curatedBy", 
                            "derivedFrom", 
                            "importedBy", 
                            "importedFrom", 
                            "providedBy", 
                            "retrievedBy", 
                            "retrievedFrom", 
                            "sourceAccessedBy"
                        ], 
                        "title": "contribution", 
                        "description": ""
                    }
                }, 
                "orcid": {
                    "type": "string", 
                    "title": "orcid", 
                    "description": "", 
                    "examples": [
                        "https://orcid.org/0000-0003-1409-4549"
                    ], 
                    "format": "uri"
                }
            }
    }, 
    "uri": {
            "type": "object",
            "additionalProperties": false,
            "required": [ "address" ],
            "properties": { 
                "address": {
                  "type": "string",
                  "format": "uri"
                },
                "access_time": {
                    "type": "string",
                    "format": "date-time"
                },
                "sha1_chksum": {
                    "type": "string",
                    "pattern": "[A-Za-z0-9]+"
                }
            } 
        }
  },
  "properties": {
    "BCO_id": {
      "$ref": "#/definitions/BCO_id"
    },
    "type": {
      "type": "string",
      "title": "type",
      "description": "",
      "examples": [
        "antiviral_resistance_detection"
      ],
      "readOnly": true
    },
    "digital_signature": {
      "type": "string",
      "title": "digital_signature",
      "description": "",
      "examples": [
        "905d7fce3f3ac64c8ea86f058ca71658"
      ],
      "readOnly": true,
      "pattern": "^([A-Za-z0-9]+)$"
    },
    "bco_spec_version": {
      "type": "string",
      "title": "bco_spec_version",
      "description": "",
      "examples": [
        "v1.2"
      ],
      "readOnly": true
    },
    "provenance_domain":{
        "$ref":"provenance_domain.json"
    },
    "usability_domain":{
        "$ref":"usability_domain.json"
    },
    "execution_domain":{
        "$ref":"execution_domain.json"
    },
    "description_domain":{
        "$ref":"description_domain.json"
    },
    "extension_domain": {
        "type": "object",
        "title": "extension_domain",
        "description": "",
        "required": [
            "fhir_extension",
            "scm_extension"
        ],
        "properties": {
            "fhir_extension": {
                "$ref":"fhir_extension.json"
            },
            "scm_extension": {
                "$ref":"scm_extension.json"
            }
        }
    },
    "parametric_domain":{
        "$ref":"parametric_domain.json"
    }
  }
}
}
