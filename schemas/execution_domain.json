{
    "type": "object", 
    "title": "execution_domain", 
    "description": "", 
    "default": null, 
    "required": [
        "script_access_type", 
        "script", 
        "script_driver", 
        "pipeline_version", 
        "platform", 
        "software_prerequisites", 
        "domain_prerequisites", 
        "env_parameters"
    ], 
    "properties": {
        "script_access_type": {
            "type": "string", 
            "enum": [
                "URI", 
                "text"
            ], 
            "title": "script_access_type", 
            "description": ""
        }, 
        "script_driver": {
            "type": "string", 
            "title": "script_driver", 
            "description": "", 
            "examples": [
                "hive", 
                "cwl-runner", 
                "shell"
            ]
        }, 
        "pipeline_version": {
            "type": "string", 
            "title": "pipeline_version", 
            "description": "", 
            "examples": [
                "2.0"
            ]
        }, 
        "platform": {
            "type": "string", 
            "title": "platform", 
            "description": "", 
            "examples": [
                "hive"
            ]
        }, 
        "script": {
            "type": "array",
            "title": "script",
            "description": "",
            "items": {
                "type": "string",
                "title": "script",
                "description": "",
                "examples": [
                    "https://example.com/workflows/antiviral_resistance_detection_hive.py"
                ]
            }
        },
        "software_prerequisites": {
            "type": "array", 
            "title": "software_prerequisites", 
            "description": "", 
            "items": {
                "type": "object", 
                "title": "software_prerequisite", 
                "description": "", 
                "required": [
                    "name", 
                    "version", 
                    "uri"
                ], 
                "properties": {
                    "name": {
                        "type": "string", 
                        "title": "name", 
                        "description": "", 
                        "examples": [
                            "HIVE-hexagon"
                        ]
                    }, 
                    "version": {
                        "type": "string", 
                        "title": "version", 
                        "description": "", 
                        "examples": [
                            "babajanian.1"
                        ]
                    }, 
                    "uri": {
                        "type": "object", 
                        "title": "uri", 
                        "description": "", 
                        "required": [
                            "address", 
                            "access_time"
                        ], 
                        "properties": {
                            "address": {
                                "type": "string", 
                                "title": "address", 
                                "description": "", 
                                "examples": [
                                    "https://hive.biochemistry.gwu.edu/dna.cgi?cmd=dna-hexagon&cmdMode=-"
                                ]
                            }, 
                            "access_time": {
                                "type": "string", 
                                "title": "access_time", 
                                "description": "", 
                                "format": "date-time"
                            }
                        }
                    }
                }
            }
        }, 
        "domain_prerequisites": {
            "type": "array", 
            "title": "domain_prerequisites", 
            "description": "", 
            "items": {
                "type": "object", 
                "title": "domain_prerequisite", 
                "description": "", 
                "required": [
                    "name", 
                    "url"
                ], 
                "properties": {
                    "name": {
                        "type": "string", 
                        "title": "name", 
                        "description": "", 
                        "examples": [
                            "HIVE"
                        ]
                    }, 
                    "url": {
                        "type": "string", 
                        "title": "url", 
                        "description": "", 
                        "default": "", 
                        "examples": [
                            "https://hive.biochemistry.gwu.edu/dna.cgi?cmd=login"
                        ]
                    }
                }
            }
        }, 
        "env_parameters": {
            "type": "array", 
            "title": "env_parameters", 
            "description": "", 
            "items": {
                "type": "object",
                "title": "env_parameter",
                "description": "",
                "required": [
                    "param",
                    "value"
                ],
                "properties": {
                    "param":{
                        "type": "string",
                        "title": "param",
                        "description": "",
                        "default": "",
                        "examples": [
                            "HOSTTYPE"
                        ]
                    },
                    "value": {
                        "type": "string",
                        "title": "value",
                        "description": "",
                        "examples": [
                            "x86_64-linux"
                        ]
                    }
                }
            }
        }
    }
}
